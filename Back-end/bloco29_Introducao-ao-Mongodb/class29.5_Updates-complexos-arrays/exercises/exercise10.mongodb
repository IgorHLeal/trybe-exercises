use ('cinema');

db.movies.updateOne(
  { title: { $eq: 'Batman' } },
  {
    $push: {
      cast: {
        $each: [
          { "character": "Batman" },
          { "character": "Alfred" },
          { "character": "Coringa" },
        ],
      },
    },
  },
);

db.movies.find();